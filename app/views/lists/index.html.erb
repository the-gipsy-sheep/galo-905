<div class="text-uppercase justify-content-center flex-column bg-dark d-flex align-items-center" style="height: 200px; color: white; font-size: 1.8em;">
  <h1 style="">Listas</h1>
  <%= link_to "Crear Lista", new_list_path, class: "btn btn-primary" %>
</div>
<div class="container">
  <div class="row">
    <ul class="list-group my-4">
      <% @lists.all.each do |list| %>
        <%= render 'list_card', list: list %>
      <% end %>
    </ul>
  </div>
</div>
